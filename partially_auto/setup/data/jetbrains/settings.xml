<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <profiles>
        <profile>
            <id>artifactory-pro</id>
            <repositories>
                <repository>
                    <id>ext-approved-local</id>
                    <name>ext-approved-local</name>
                    <url>http://repo.vmo.lab:8080/artifactory/ext-approved-local</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>build-repos</id>
                    <name>build-repos</name>
                    <url>http://repo.vmo.lab:8080/artifactory/build-repos</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>vce.release</id>
                    <name>vce.release</name>
                    <url>http://repo.vmo.lab:8080/artifactory/libs-release-local</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>vce.snapshot</id>
                    <name>vce.snapshot</name>
                    <url>http://repo.vmo.lab:8080/artifactory/libs-snapshot-local</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>vce.plugins</id>
                    <name>plugins-release</name>
                    <url>http://repo.vmo.lab:8080/artifactory/plugins-release-local</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>vce.plugins.snapshots</id>
                    <name>plugins-snapshot</name>
                    <url>http://repo.vmo.lab:8080/artifactory/plugins-snapshot-local</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
